(* Success: 1 *)
data mutable pool a = P adopts cell a
 and mutable cell a = Cell { contents: a }
val () =
  let c = Cell { contents = 3 } in
  let p1 = P in
  let p2 = P in
  give (c : cell int) to p1;
  if p2 adopts c then fail;
  take c from p1;
  give (c : cell int) to p2;
  if p1 adopts c then fail;
  take c from p2;
  if p2 adopts c then fail;
  print 1
